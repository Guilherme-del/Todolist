{
  "expo": {
    "name": "TodoListApp",
    "slug": "TodoListApp",
    "version": "0.0.1",
    "orientation": "portrait",
    "icon": "./app/assets/img/icon.png",
    "splash": {
    "image": "./app/assets/img/splash.png",
    "resizeMode": "contain",
    "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/img/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./app/assets/img/favicon.png"
    }
  }
}
